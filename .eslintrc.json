{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-alert": "off",
        "prettier/prettier": [
            "error",
        {
            "singleQuote": true,
            "semi": false,
            "printWidth": 80,
            "tabWidth": 2,
            "endOfLine": "auto"
        }
    ]
    }
}
